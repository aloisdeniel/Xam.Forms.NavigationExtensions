<?xml version="1.0" encoding="UTF-8"?>
<ContentPage Title="Albums" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Sample.Navigation.AlbumsPage">
	<ListView ItemsSource="{Binding Albums}" ItemTapped="OnItemTapped">
		<ListView.ItemTemplate>
			<DataTemplate>
				<ViewCell>
					<Label Margin="10" Text="{Binding Title}" />
				</ViewCell>
			</DataTemplate>
		</ListView.ItemTemplate>
	</ListView>
</ContentPage>
